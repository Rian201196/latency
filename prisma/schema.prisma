generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Sensor {
  sensor_id     Int      @id @default(autoincrement())
  kode           String
  nama           String
  lat            Float
  long           Float
  kota           String
  provinsi       String
  tipe           String
  kategori       String
  upt            String
  last_latency   String?  // NULLABLE
  status         String?  // NULLABLE
  created_at     DateTime @default(now())
}